-- documantation for calcing --

import calcing
calcing.plus(numb1=1, numb2=5) # --  1 + 5 = 6
calcing.minus(numb1=1, numb2=5) # --  1 - 5 = -4
calcing.dil(numb1=1, numb2=5) # -- 1 / 5 = 0.2
calcing.diln(numb1=1, numb2=5) # -- 1 // 5 = 0


