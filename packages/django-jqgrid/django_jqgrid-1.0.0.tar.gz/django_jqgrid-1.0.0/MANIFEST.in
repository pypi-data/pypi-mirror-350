include README.md
include LICENSE
include CHANGELOG.md
include CONTRIBUTING.md
include pyproject.toml
include setup.py
include MANIFEST.in

recursive-include django_jqgrid *.py
recursive-include django_jqgrid/static *
recursive-include django_jqgrid/templates *
recursive-include django_jqgrid/management *
recursive-include django_jqgrid/migrations *
recursive-include django_jqgrid/locale *

recursive-exclude * __pycache__
recursive-exclude * *.py[co]
recursive-exclude * .DS_Store
recursive-exclude * *.swp
recursive-exclude * *~

prune tests
prune docs
prune examples
prune .github