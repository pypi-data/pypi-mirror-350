# WebInfo Retriever Configuration
# Copy this file to .env and fill in your values

# Required: Google Gemini API Key
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Scraping Configuration
WEBINFO_TIMEOUT=30
WEBINFO_MAX_RETRIES=3
WEBINFO_USER_AGENT=WebInfo-Retriever/1.0.0

# Optional: Cache Configuration
WEBINFO_CACHE_TTL=3600

# Optional: Rate Limiting
WEBINFO_RATE_LIMIT_RPM=60

# Note: The search feature uses only the Gemini API for intelligent URL discovery
# No additional search engine API keys are required
