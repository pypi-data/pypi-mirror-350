<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_impact_data_type_uom_form" model="ir.ui.view">
        <field name="name">impact.data.type.uom.form</field>
        <field name="model">impact.data.type.uom</field>
        <field name="arch" type="xml">
            <form string="Impact Data Type UOM">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_impact_data_type_uom_tree" model="ir.ui.view">
        <field name="name">impact.data.type.uom.tree</field>
        <field name="model">impact.data.type.uom</field>
        <field name="arch" type="xml">
            <tree string="Impact Data Type UOMs">
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="view_impact_data_type_uom_search" model="ir.ui.view">
        <field name="name">impact.data.type.uom.search</field>
        <field name="model">impact.data.type.uom</field>
        <field name="arch" type="xml">
            <search string="Search Impact Data Type UOMs">
                <field name="name"/>
            </search>
        </field>
    </record>
    <record id="action_impact_data_type_uom" model="ir.actions.act_window">
        <field name="name">Impact Data Type UOMs</field>
        <field name="res_model">impact.data.type.uom</field>
        <field name="view_mode">tree,form</field>
        <field name="search_view_id" ref="view_impact_data_type_uom_search"/>
        <field name="view_id" ref="view_impact_data_type_uom_tree"/>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Click to create a new impact data type UOM.
            </p>
        </field>
    </record>
</odoo>
