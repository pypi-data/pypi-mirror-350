<odoo>
<data noupdate="1">
<record id="recruitment_vacancy_sequence_template" model="sequence.template">
    <field name="name">Standard</field>
    <field name="model_id" ref="model_recruitment_vacancy" />
    <field name="initial_string">/</field>
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','recruitment_vacancy'),('name','=','name')]"
            />
    <field
                name="date_field_id"
                search="[('model_id.model','=','recruitment_vacancy'),('name','=','date_start')]"
            />
    <field name="computation_method">use_python</field>
    <field name="python_code">result=True</field>
    <field name="sequence_id" ref="sequence_recruitment_vacancy" />
    <field name="sequence_selection_method">use_sequence</field>
    <field name="add_custom_prefix" eval="0" />
    <field name="add_custom_suffix" eval="0" />
</record>
</data>
</odoo>
