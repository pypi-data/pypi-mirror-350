<odoo>
<data noupdate="1">
<!-- APPLICANT -->
<record id="sequence_recruitment_applicant" model="ir.sequence">
    <field name="name">Applicant</field>
    <field name="code">recruitment_applicant</field>
    <field name="prefix">REC-APP/%(range_year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>
<!-- VACANCY -->
<record id="sequence_recruitment_vacancy" model="ir.sequence">
    <field name="name">Vacancy</field>
    <field name="code">recruitment_vacancy</field>
    <field name="prefix">REC-VAC/%(range_year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>
</data>
</odoo>
