                        "default": "spectype"
                      },
                      "unit": {
                        "type": "string",
                        "default": ""
                      },
                      "notation": {
                        "type": "string",
                        "default": "text"
                      },
                      "decimal": {
                        "type": "number",
                        "default": null
                      },
                      "figurePath": {
                        "type": "string",
