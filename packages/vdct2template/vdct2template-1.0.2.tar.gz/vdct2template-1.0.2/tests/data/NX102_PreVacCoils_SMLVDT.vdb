#
record(stringout, "$(device)$(lvdtprefix=:LVDT)$(lvdtID):LONGDESC") {
  field(DESC, "Long description of LVDT $(lvdtID)")
  field(PINI, "YES")
  field(VAL, "$(lvdt_longdesc=Manipulator LVDT $(lvdtID))")
}

